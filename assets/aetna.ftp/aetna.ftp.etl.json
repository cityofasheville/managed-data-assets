{
    "run_group": "aetna",
    "tasks": [
      {
        "type": "table_copy",
        "description": "Copy Aetna to S3",
        "source_location": {
          "connection": "munis/munprod/fme_jobs",
          "schemaname": "avl",
          "tablename": "PR_AETNA_Feed"
        },
        "target_location": {
          "connection": "s3_data_files",
          "filename": "aetna_asheville_${YYYY}${MM}${DD}.csv",
          "s3_path": "aetna/"
        },
        "active": true
      },
      {
        "type": "sftp",
        "description": "Copy Aetna S3 to FTP site",
        "action": "put",
        "s3_connection": "s3_data_files",
        "s3_path": "aetna/",
        "ftp_connection": "aetna_ftp",
        "ftp_path": "/AetnaSRC_Reports/toAetna/",
        "filename": "aetna_asheville_${YYYY}${MM}${DD}.csv",
        "active": true
      }
    ]
}
